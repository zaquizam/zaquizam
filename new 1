SELECT  IdSemana,  Semana  FROM  ss_Semana  WHERE  IdSemana in ( 57,56,55,54,53) Order By  IdSemana

SELECT  Id_Indicador,  Abreviatura,  UnidadMedida  FROM  RS_Indicadores  WHERE  Ind_Activo = 1  And Id_Indicador in (6) ORDER BY  Id_Indicador

SELECT  Id_Area,  Area,  Id_Zona,  Zona,  Id_Canal,  Canal,  Id_Fabricante,  Fabricante,  Id_Marca,  Marca,  Id_Segmento,  Segmento,  Id_Tamano,  
        Tamano,  CodigoBarra,  Descripcion,  UnidadMedida,  VentasUni,  VentasVal,  VentasUniMed,  VentasNo,  DistribucionNum,  DistribucionPon,
		DistribucionEfe,  ShareUni,  ShareVol,  ShareVal,  PrecioPro,  PrecioMax,  PrecioMin,  PrecioUni,  PrecioUniMed,  id_Semana
		FROM
		RS_DataProcSem
		WHERE
		Id_Categoria = 1
		And Id_Semana in ( 57,56,55,54,53)
		And Id_Area in (0)
		And Id_Zona in (0)
		And Id_Canal in (0)
		And Id_Fabricante in (0,1,2,3,4,8,10,12,13)
		And Id_Marca in (1,5,10,11,13,16,18,19,21,23,26,27,28,30,31,34,35,36,40,42,43,45,48,54)
		And Id_Segmento in (0)
		And Id_Tamano in (14,15,16,18,5,19,3834,8,9,12,13)
		And CodigoBarra in ('7591031003588','7591031100041','7811403','7591127703507','7591127703606','7591127704542','7591127714541','7702535013922','7591127103505','7591127103659','7702535001929','7591127103369','7591127103390','7591127803603','7591127113320','7591127113566','7591127113382','7591127114549','7591127114402','07831504','7592134020182','7592134020380','7591127753502','7591127753601','7591127754547','7591127854469','7591127853325','7591127853387','7592661000022','7592661000039','7592661000053','7592661000046','7592661000107','7592661000169','10012925','7591031101895','7591031000136','7591031000037','7591031003403','7591031003359','7591031000228','7591031003397','7591031003465','7591031003434','7591031003496','7591031100355','7591031000204','7591127603500','7591127613509','7591127603609','7591127604545','7591127604408','7591127863386','7591127863324','7591127883865','7592134043037','7591025000067','7591031003243','7591031100539','7591031100522','7591031003595','7591031003557','7591031003526','7592103000535','7591127713551','7591127713605','7591127714404','7591127713322','7591127104540','7591127122520','7591127104403','04963406','7591127803658','7591127804402','7591127123527','7591127123329','7591127123626','7591127113627','7592103000542','7591127853554','7591127853622','7591127854407','7592661001210','7592661000015','7592661000077','7592661001197','7592661000947','7592661000138','7591031003366','7591031001324','7591031000211','7591031001089','7591031001218','7591031001201','7591031003373','7591031000013','7591031000020','7591127863553','7591127863621','7591127864406','7591127883551','7591031003250','7591031000983','7591031003267','7591031003236','7591031102069','7591031003229','7591031003328','7591031003281','7591031102052','7702090064186','049000061062','8595004610420') ORDER BY  Id_Area,  Id_Zona,  Id_Canal,  Id_Fabricante,  Id_Marca,  Id_Segmento,  Id_Tamano,  CodigoBarra,  Descripcion,  id_Semana