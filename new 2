								GetCambioCombo(9,jQuery(cbo).val());					
							}
						});
						
					})
					.fail (function(jqXHR, textStatus, errorThrown) {
						console.log('Error GCC-Marca opc-5');
						swal("Algo salio mal.!","GCC-Marca opc-5", "error");
					});
										
				})
				.fail (function(jqXHR, textStatus, errorThrown) {
					console.log('Error GCC-Marca opc-5');
					swal("Algo salio mal.!","GCC-Marca opc-5", "error");
				});

				
			})
